import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        
        int marks;
        marks=s.nextInt();
        if(marks>=90){
            System.out.print("A");
        }
        else if(marks>=75){
            System.out.print("B");
        }
        else if(marks>=60){
            System.out.print("c");
        }
        else{
            System.out.print("Fail");
        }
    }
}
