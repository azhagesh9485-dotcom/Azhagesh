public class Solution {

    public static void main(String[] args) {
    Scanner S=new Scanner (System. in);
        
    int N=S.nextInt();
        if( N>0){
            System.out.print("Positive ");
    }
        else{
            System.out.print("Negative ");
        
            
}
    }
    }
