import java.io.*;
import java.util.*;

public class Pattern {

    public static void main(String[] args) {
    Scanner S=new Scanner (System.in);
    int N=S.nextInt();
        for(int i=0;i<N;i++){
            for(int j=0;j<N;j++){
                System.out.print("*");
              }
            System.out.println();
    }
        }
    }
