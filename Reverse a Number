import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System. in);
      int N;
      int rev;
        N=s.nextInt();
        rev=0;
        while(N>0){
            rev=rev*10+N%10;
            N=N/10;
            }
        System.out.print(rev);
        }
    }
